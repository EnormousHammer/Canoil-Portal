{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/((?!.*\\.(ico|png|jpg|jpeg|gif|svg|css|js|json|woff|woff2|ttf|eot|pdf|mp4|webm|avif|webp)).*)",
      "destination": "/index.html"
    }
  ]
}
